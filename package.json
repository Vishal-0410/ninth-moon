{
  "name": "moon-ai-project",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "node build.js",
    "dev:build": "npm run type-check && node build.js",
    "start:api": "node dist/index.js",
    "start:account-worker": "node dist/workers/accountWorker.js",
    "start:notification-worker": "node dist/workers/notificationWorker.js",
    "start:workers": "concurrently \"npm run start:account-worker\" \"npm run start:notification-worker\"",
    "start": "concurrently \"npm run start:api\" \"npm run start:workers\"",
    "seed": "node dist/seed/seedFeature.js"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.4.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "timezones-list": "^3.1.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/date-fns": "^2.5.3",
    "@types/express": "^5.0.3",
    "@types/google-cloud__text-to-speech": "^0.5.2",
    "@types/node": "^20.19.13",
    "@types/nodemailer": "^7.0.1",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "esbuild-node-externals": "^1.18.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  }
}
